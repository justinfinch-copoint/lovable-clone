{
  "name": "gameable",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "agents:start": "cd ../agents-service && python -m chainlit run app.py --headless --port 8000",
    "agents:api": "cd ../agents-service && python api_server.py",
    "dev:full": "concurrently \"npm run dev\" \"npm run agents:start\" \"npm run agents:api\"",
    "dev:with-agents": "npm run dev:full"
  },
  "dependencies": {
    "@chainlit/react-client": "^0.2.4",
    "dotenv": "^16.0.0",
    "next": "14.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "recoil": "^0.7.7"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "autoprefixer": "^10.0.0",
    "concurrently": "^9.2.0",
    "eslint": "^8.0.0",
    "eslint-config-next": "14.0.0",
    "postcss": "^8.0.0",
    "tailwindcss": "^3.3.0",
    "typescript": "^5.0.0"
  }
}
